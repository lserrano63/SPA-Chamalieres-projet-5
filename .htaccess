RewriteEngine On
RewriteBase /

RewriteRule ^Acceuil$ SPA-Chamalieres/index.php [L]
RewriteRule ^Contact$ SPA-Chamalieres/index.php?action=contact [L]
RewriteRule ^Nos-Animaux$ SPA-Chamalieres/index.php?action=viewAnimals [L]
RewriteRule ^Actualités/Page-([0-9]+)$ SPA-Chamalieres/index.php?action=viewPosts&page=$1 [L]
RewriteRule ^Administration$ SPA-Chamalieres/index.php?action=admin [L]
RewriteRule ^Déconnexion$ SPA-Chamalieres/index.php?action=disconnect [L]
RewriteRule ^Mentions-Légales$ SPA-Chamalieres/index.php?action=mentions [L]
RewriteRule ^Connection$ SPA-Chamalieres/index.php?action=login [L]
RewriteRule ^Nouveau-Post$ SPA-Chamalieres/index.php?action=postCreation [L]
RewriteRule ^Administration-Post/Page-([0-9]+)$ SPA-Chamalieres/index.php?action=adminPost&page=1 [L]
RewriteRule ^Ajouter-Animal$ SPA-Chamalieres/index.php?action=formAnimal [L]
RewriteRule ^Administration-Animaux$ SPA-Chamalieres/index.php?action=adminAnimal [L]
RewriteRule ^Administration-Commentaires$ SPA-Chamalieres/index.php?action=viewComments [L]
RewriteRule ^Ajouter-Administrateur$ SPA-Chamalieres/index.php?action=manageRights [L]
RewriteRule ^Profil$ SPA-Chamalieres/index.php?action=viewProfile [L]
RewriteRule ^Posts/Post-([0-9]+)$ SPA-Chamalieres/index.php?action=viewPost&id=$1 [L]
RewriteRule ^Animaux/Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=viewAnimal&id=$1 [L]
RewriteRule ^Signalement/Commentaire-([0-9]+)-Post-([0-9]+)$ SPA-Chamalieres/index.php?action=reportedCommentPost&id=$1&post_id=$2 [L]
RewriteRule ^Ajout-Commentaire/Post-([0-9]+)$ SPA-Chamalieres/index.php?action=addComment&id=$1 [L]
RewriteRule ^Signalement/Commentaire-([0-9]+)-Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=reportedCommentAnimal&id=$1&animal_id=$2 [L]
RewriteRule ^Ajout-Commentaire/Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=addCommentAnimal&id=$1 [L]
RewriteRule ^Modification-Post-([0-9]+)$ SPA-Chamalieres/index.php?action=adminPostModify&id=$1 [L]
RewriteRule ^Suppression-Post-([0-9]+)$ SPA-Chamalieres/index.php?action=adminPostDelete&id=$1 [L]
RewriteRule ^Post-Modifié-([0-9]+)$ SPA-Chamalieres/index.php?action=adminPostModified&id=$1 [L]
RewriteRule ^Ajout-Post$ SPA-Chamalieres/index.php?action=addPost [L]
RewriteRule ^Ajout-Animal$ SPA-Chamalieres/index.php?action=addAnimal [L]
RewriteRule ^Modification-Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=adminAnimalModify&id=$1 [L]
RewriteRule ^Suppression-Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=adminAnimalDelete&id=$1 [L]
RewriteRule ^Animal-Modifié-([0-9]+)$ SPA-Chamalieres/index.php?action=adminAnimalModified&id=$1 [L]
RewriteRule ^Ajouter-Droits$ SPA-Chamalieres/index.php?action=addRights [L]
RewriteRule ^Modification-Profil$ SPA-Chamalieres/index.php?action=modifyAdminProfile [L]
RewriteRule ^Accepter-Commentaire-Post-([0-9]+)$ SPA-Chamalieres/index.php?action=accept&id=$1 [L]
RewriteRule ^Suppression-Commentaire-Post-([0-9]+)$ SPA-Chamalieres/index.php?action=remove&id=$1 [L]
RewriteRule ^Accepter-Commentaire-Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=accept&id=$1 [L]
RewriteRule ^Suppression-Commentaire-Animal-([0-9]+)$ SPA-Chamalieres/index.php?action=remove&id=$1 [L]